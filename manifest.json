{
  "manifest_version": 3,
  "name": "coding_masters util key",
  "version": "0.3",
  "description": "실행: alt+1, 채점하기: alt+2, 저장하기: alt+3, 제출하기: alt+4 코딩마스터 열기: alt+`",
  "content_scripts": [
    {
      "matches": [
        "https://*.aivle.edu.kt.co.kr/*",
        "https://*.coding-ai.aivle-edu.kr/*"
      ],
      "js": ["index.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/aivle_16.png",
      "32": "/images/aivle_32.png",
      "48": "/images/aivle_48.png",
      "128": "/images/aivle_128.png"
    }
  },
  "icons": {
    "16": "/images/aivle_16.png",
    "32": "/images/aivle_32.png",
    "48": "/images/aivle_48.png",
    "128": "/images/aivle_128.png"
  }
}
